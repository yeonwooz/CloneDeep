{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./build-tsc",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "allowJs": true
    },
    "include": ["src/**/*.ts", "src/cloneDeep.ts"],
    "exclude": ["node_modules"]
  }